<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.finmate.member.mapper.UserMapper">


<insert id="insertUser" parameterType="org.finmate.member.domain.UserVO">
        INSERT INTO user (name, account_id, email, password, birth, provider, create_at)
        VALUES (#{name}, #{accountId}, #{email}, #{password}, #{birth}, #{provider}, #{createAt})
    </insert>
    <delete id="deleteByAccountId" parameterType="String">
        DELETE FROM user WHERE account_id = #{accountId}
    </delete>

</mapper>
