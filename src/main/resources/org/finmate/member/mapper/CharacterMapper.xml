<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.finmate.member.mapper.CharacterMapper">

    <select id="getCharacterById" resultType="org.finmate.member.domain.CharacterVO">
        SELECT id, animal_name, animal_image_1, animal_image_2, animal_image_3, animal_image_4
        FROM animal_character c
        WHERE c.id = #{id}
    </select>

</mapper>

